{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ESNext",
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": false,
        "declaration": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "sourceMap": true,
        "removeComments": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "incremental": true
    }
}
